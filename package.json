{
  "name": "universal-pokec-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "start:prod": "./node_modules/.bin/better-npm-run start:prod",
    "start:dev": "./node_modules/.bin/better-npm-run start:dev",
    "start:hot": "./node_modules/.bin/better-npm-run start:hot"
  },
  "betterScripts": {
    "start:prod": {
      "command": "node ./src/server.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./src",
        "PORT": "8080"
      }
    },
    "start:dev": {
      "command": "nodemon ./src/server.js",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src",
        "PORT": "8080"
      }
    },
    "start:hot": {
      "command": "node webpack/hot-server",
      "env": {
        "UV_THREADPOOL_SIZE": "100",
        "NODE_PATH": "./src",
        "PORT": "8080"
      }
    }
  },
  "dependencies": {
    "axios": "^0.8.1",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-react-constant-elements": "^6.3.13",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-runtime": "^6.5.0",
    "better-npm-run": "0.0.7",
    "bluebird": "^3.1.1",
    "clean-webpack-plugin": "^0.1.6",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "history": "2.0.0-rc2",
    "pretty-error": "^2.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-helmet": "^2.2.0",
    "react-redux": "^4.0.6",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "react-simple-async": "^0.3.0",
    "redux": "^3.0.5",
    "redux-axios-middleware": "0.0.2",
    "redux-form": "^4.1.1",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-isomorphic-tools": "^2.2.24"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.0",
    "clean-webpack-plugin": "^0.1.6",
    "nodemon": "^1.8.1",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
